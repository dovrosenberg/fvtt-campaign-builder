{
  "id": "campaign-builder",
  "title": "World & Campaign Builder",
  "description": "Plan your campaigns and create full worlds (like Kanka, WorldAnvil, etc. but simpler) right inside Foundry - as you play.",

  "compatibility": {
    "minimum": "13",
    "verified": "13.344"
  },

  "esmodules": [
    "scripts/index.js"
  ],
  "styles": [
    "styles/campaign-builder.css"
  ],
  "packs": [],

  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
   ],

   "documentTypes": {
    "JournalEntryPage": {
      "entry": {},
      "session": {
        "htmlFields": [ "description" ]
      },
      "pc": {
        "htmlFields": [ "background", "plotPoints", "magicItems" ]
      }
    }
  },

  "authors": [
    {
      "name": "Dov Rosenberg",
      "#discord": "",
      "#email": "",
      "#url": ""
    }
  ],

  "version": "#{VERSION}#",
  "url": "https://github.com/dovrosenberg/fvtt-campaign-builder",
  "manifest": "https://github.com/dovrosenberg/fvtt-campaign-builder/releases/latest/download/module.json",
  "download": "#{DOWNLOAD}#",

  "license": "https://github.com/dovrosenberg/fvtt-campaign-builder/blob/master/LICENSE.md",
  "readme": "https://github.com/dovrosenberg/fvtt-campaign-builder/blob/master/README.md",
  "bugs": "https://github.com/dovrosenberg/fvtt-campaign-builder/issues",
  "changelog": "https://github.com/dovrosenberg/fvtt-campaign-builder/blob/master/CHANGELOG.md"
}
