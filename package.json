{
  "name": "fvtt-world-builder",
  "title": "Foundry VTT World Builder",
  "version": "0.0.1",
  "description": "Create regions, organizations, culture, and other elements of your world, right in Foundry.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dovrosenberg/fvtt-world-builder.git"
  },
  "author": "Dov Rosenberg",
  "homepage": "https://github.com/dovrosenberg/fvtt-world-builder#readme",
  "main": "main.ts",
  "scripts": {
    "build": "vite build",
    "debug": "NODE_ENV=development vite build --mode development --minify=false",
    "lint": "eslint --ext .ts,.vue src",
    "copy": "rm -rf $FOUNDRYVTT_DATA/Data/modules/world-builder/* && cp -r $(pwd)/dist/* $FOUNDRYVTT_DATA/Data/modules/world-builder",
    "linkdata": "ln -s $PWD/dist $FOUNDRYVTT_DATA/Data/modules/world-builder",
    "tsc": "tsc --skipLibCheck -p ./tsconfig.json",
    "test": "jest",
    "coverage": "jest --coverage",
    "unlinkdata": "unlink $FOUNDRYVTT_DATA/Data/modules/world-builder",
    "buildcopy": "npm run build && npm run copy",
    "foundry": "node $FOUNDRYVTT_APP/resources/app/main.js"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@league-of-foundry-developers/foundry-vtt-types": "github:League-of-Foundry-Developers/foundry-vtt-types#main",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@primevue/auto-import-resolver": "^4.2.5",
    "@types/jest": "^29.5.14",
    "@types/jquery": "^3.5.32",
    "@types/lodash": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.9.2",
    "node-sass": "^9.0.0",
    "null-loader": "^4.0.1",
    "prettier": "^3.4.2",
    "raw-loader": "^4.0.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.83.0",
    "sass-loader": "^16.0.4",
    "semver-increment": "^1.0.1",
    "source-map-loader": "^5.0.0",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.6",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-plugin-html": "3.2.2"
  },
  "dependencies": {
    "@imengyu/vue3-context-menu": "^1.4.4",
    "@primevue/themes": "^4.2.5",
    "pinia": "^2.3.0",
    "primevue": "^4.2.5",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^9.4.3"
  }
}
