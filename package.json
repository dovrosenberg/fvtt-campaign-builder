{
  "name": "fvtt-world-builder",
  "title": "Foundry VTT World Builder",
  "version": "0.0.1",
  "description": "Create regions, organizations, culture, and other elements of your world, right in Foundry.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dovrosenberg/fvtt-world-builder.git"
  },
  "author": "Dov Rosenberg",
  "homepage": "https://github.com/dovrosenberg/fvtt-world-builder#readme",
  "main": "main.ts",
  "scripts": {
    "build": "vite build",
    "debug": "NODE_ENV=development vite build --mode development --minify=false",
    "lint": "eslint --ext .ts,.vue src",
    "copy": "rm -rf $FOUNDRYVTT_DATA/Data/modules/world-builder/* && cp -r $(pwd)/dist/* $FOUNDRYVTT_DATA/Data/modules/world-builder",
    "linkdata": "ln -s $PWD/dist $FOUNDRYVTT_DATA/Data/modules/world-builder",
    "tsc": "tsc --noEmit --skipLibCheck",
    "test": "jest",
    "coverage": "jest --coverage",
    "unlinkdata": "unlink $FOUNDRYVTT_DATA/Data/modules/world-builder",
    "buildcopy": "npm run build && npm run copy",
    "foundry": "node $FOUNDRYVTT_APP/resources/app/main.js",
    "foundry10": "node ~/foundry10/resources/app/main.js --dataPath=/home/dov/foundry10data"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@league-of-foundry-developers/foundry-vtt-types": "github:League-of-Foundry-Developers/foundry-vtt-types#main",
    "@originjs/vite-plugin-commonjs": "^1.0.1",
    "@quasar/vite-plugin": "^1.7.0",
    "@types/jest": "^29.5.10",
    "@types/jquery": "^3.5.29",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "css-loader": "^6.4.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^9.27.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.4.2",
    "node-sass": "^9.0.0",
    "null-loader": "^4.0.1",
    "prettier": "^2.4.1",
    "raw-loader": "^4.0.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.45.0",
    "sass-loader": "^13.3.2",
    "semver-increment": "^1.0.1",
    "source-map-loader": "^3.0.0",
    "string-replace-loader": "^3.0.3",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.2.6",
    "vite": "^5.2.0",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-html": "3.2.2"
  },
  "dependencies": {
    "@imengyu/vue3-context-menu": "^1.4.1",
    "pinia": "^2.1.7",
    "quasar": "^2.16.6",
    "vue": "^3.4.31",
    "vue-eslint-parser": "^9.4.3"
  }
}
